<view><view class="nl_income-list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="key" wx:key="key"><view class="nl_income-item"><block wx:if="{{item.type===1}}"><view class="nl_income-amount nl_color-success">{{"+"+item.coin_count+''}}<text class="nl_income-desc">{{item.source_desc}}</text></view></block><block wx:else><block wx:if="{{item.type===2}}"><view class="nl_income-amount nl_color-primary">{{"-"+item.coin_count+''}}<text class="nl_income-desc">{{item.source_desc}}</text></view></block></block><view class="nl_income-time">{{item.created_at}}</view><view class="nl_income-type"><block wx:if="{{item.type===1}}"><text class="nl_color-success">收入</text></block><block wx:else><block wx:if="{{item.type===2}}"><text class="nl_color-primary">支出</text></block></block></view></view></block></view></view>