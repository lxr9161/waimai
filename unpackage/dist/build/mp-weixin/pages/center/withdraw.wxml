<view><view class="nl_withdrawal-way"><block wx:if="{{withdrawalWay.account}}"><view class="nl_relative"><view class="nl_text-secondary">提现到</view><view class="nl_mt-10"><block wx:if="{{withdrawalWay.way===2}}"><text class="nl_text-secondary nl_mr-20">支付宝</text></block><block wx:else><block wx:if="{{withdrawalWay.way===1}}"><text class="nl_text-secondary nl_mr-20">微信</text></block></block><text>{{withdrawalWay.account}}</text></view><navigator class="nl_change-withdrawal-way" url="./third">更换账号</navigator></view></block><block wx:else><view><navigator class="nl_set-account" url="./third">还未设置提现账号，去设置 >></navigator></view></block></view><view class="nl_withdrawal"><view><view class="nl_withdrawal-form"><view class="nl_withdrawal-title">提现金额</view><view class="nl_withdrawal-input-box"><text class="nl_withdrawal-icon">¥</text><input class="nl_withdrawal-input" type="digit" data-event-opts="{{[['input',[['__set_model',['','applyNum','$event',[]]],['checkAmount',['$event']]]]]}}" value="{{applyNum}}" bindinput="__e"/></view><block wx:if="{{errorMsg===''}}"><view class="nl_balance-info"><text>{{"当前可提现余额"+leftIncome+"元，"}}</text><text data-event-opts="{{[['tap',[['setAll',['$event']]]]]}}" class="nl_withdrawal-all" bindtap="__e">全部提现</text></view></block><block wx:else><view class="nl_balance-error">{{errorMsg}}</view></block><view><button class="nl_withdrawal-btn" disabled="{{errorMsg}}" data-event-opts="{{[['tap',[['withdrawalHandle',['$event']]]]]}}" bindtap="__e">提 现</button></view></view></view><view class="nl_withdrawal-rule"><block wx:for="{{setting.rule}}" wx:for-item="item" wx:for-index="key" wx:key="key"><view class="nl_withdrawal-rule-item">{{item}}</view></block></view><view class="nl_copy-wx nl_mt-20"><button data-event-opts="{{[['tap',[['copyWx',['$event']]]]]}}" bindtap="__e">复制客服微信</button></view></view></view>