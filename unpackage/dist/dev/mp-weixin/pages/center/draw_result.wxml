<view><view class="nl_reward-list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="key" wx:key="key"><view class="nl_reward-item"><view class="nl_reward-title">{{item.reward_title}}</view><view class="nl_reward-time">{{item.created_at}}</view><view class="nl_reward-status"><block wx:if="{{item.status===0}}"><text class="nl_color-default">未发放</text></block><block wx:else><block wx:if="{{item.status===1}}"><text class="nl_color-success">已发放</text></block></block></view></view></block></view></view>